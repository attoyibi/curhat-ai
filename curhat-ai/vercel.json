{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "src/main.jsx",
        "use": "@vercel/react"
      }
    ],
    "routes": [
      {
        "src": "/((?!api).*)",  // This regex prevents routes that start with "/api" from being redirected
        "dest": "/index.html"    // Redirects all other routes to index.html
      }
    ]
  }
  